type User {
  id: ID!
  email: String
  displayName: String
  pictureURL: String
}

type Listing {
  id: Int!
  title: String!
  location: String!
  description: String!
  host: User!
  picture_url: String!
  distance_km: Float!
  rating: Float!
  price_per_night: Int!
}

type Query {
  listings: [Listing!]!
  getListing(id: Int!): Listing
}